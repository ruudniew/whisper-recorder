[run]
parallel = False
concurrency = thread
source = whisper_transcriber
omit = 
    */tests/*
    */venv/*
    */whisperlivekit/*
    */__pycache__/*
    */site-packages/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract

[html]
directory = htmlcov